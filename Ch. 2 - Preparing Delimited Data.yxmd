          <productionModeScript>#################################
from ayx import Package
import pandas as pd


#################################
from ayx import Alteryx
from datetime import datetime

# read the input
df = Alteryx.read('#1')

# rename the colum
df = df.rename(columns={'Field_1': 'Poem_to_split'})

# split the columns &amp; get rid of the single and double quotes
# the [] create a list of chars. The ^ negates the list. 
splitted_df = df['Poem_to_split'].str.replace(r'[^A-Za-z0-9 ,-]', '').str.split(',', n=3, expand=True)

# rename the columns
splitted_df = splitted_df.rename(columns={0: 'Poem', 1: 'Poem_ID', 2: 'Poem_Read_Date'})

# parse date string to datetime
splitted_df['Poem_Read_Date'] = pd.to_datetime(splitted_df['Poem_Read_Date'], dayfirst=True)

# output df to the first connection
Alteryx.write(splitted_df,1)</productionModeScript>
